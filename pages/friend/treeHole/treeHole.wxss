.treeHole_box{
    height:100%;
    width:100%;
    background-image: url("../../images/treeHole_bg.jpg");
    background-size: 100% 100%;
}
page{
height: 100%;
}

    .treeHole_text{
        padding-top: 10px;
        color: #663333;
        font-size: 0.823529rem;
    }
    .treeHole_text view{
        margin-bottom: 0.941176rem;
        text-indent:0.588rem;
        line-height: 1.4117647rem;
    }
    .treeHole_c{
        height:9.647rem;
        width:10.235rem;
        background-image: url("../../images/circular.png");
        background-size: cover;
        position: absolute;
        left:9.3rem;
        /*right: 1.823529rem;*/
        bottom:6.77647rem;
    }
    .treeHole_c text{
        color: #663333;
        font-size: 0.588235rem;
        line-height: 1.17647rem;
        display: block;
    }
    .treeHole_c view{
        position: absolute;
        top: 1.411764rem;
        left:1.88235rem;
    }
    .treeHole_bottom{
        width: 6.17647rem;
        height: 2rem;
        background: rgba(255,255,255,0.61);
        border-radius: 1rem;
        position: absolute;
        bottom: 2.117647rem;
        right:0.76471rem;
        color: #663333;
        font-size: 0.76471rem;
        line-height: 2rem;
        padding-left: 0.88235rem;
    }
.treeHole_bottom image{
    height: 0.88235rem;
    width: 0.88235rem;
    position: absolute;
    right:0.4117647rem;
    top:0.588235rem;
}
    .treeHole_bottom:active{
        background: rgba(244,199,146,1);
    }