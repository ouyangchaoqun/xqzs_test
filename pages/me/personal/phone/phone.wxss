.phone_text{
  padding-top:40px;
  color:#666666;
  font-size:14px;
  text-align: center; 
  margin-bottom: 50px;
}
.phone{
  height:40px;
  line-height:40px;
  border-top:1px solid #dcdcdc;
  border-bottom:1px solid #dcdcdc;
  font-size: 14px;
  margin-bottom:20px;
}
.phone_box{
  padding:0 20px;
}
.phone_left{
  float: left;
 padding-left: 10px;
  width:80px;
  border-right:1px solid #dcdcdc;
}
.phone_left image{
  width:15px;
  height:15px;
  margin-right:10px;
  display: block;
  float: right;
  margin-top:13px;
}
.phone_right{
  padding-left:10px;
  float: left;
}
.phone_right input{
  height:40px;
  line-height: 40px;
}
.phone_bottom{
  width:100%;
  height:40px;
  margin-bottom:30px;
}
.phone_bottom input{
  height:40px;
  line-height:40px;
  width:140px;
  background:#eeeeee;
  border-radius:5px;
  padding-left:10px;
  font-size:14px;
  float: left;
}
.phone_bottom navigator{
  width:100px;
  border:1px solid #eeeeee;
  height:40px;
  line-height:40px;
  text-align: center;
  float: right;
  border-radius: 5px;
  font-size: 14px;
  color:#dcdcdc;
}
button{
  width:100%;
 height:40px;
 line-height:40px;
}

